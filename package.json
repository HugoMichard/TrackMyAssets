  },
  "homepage": "https://github.com/HugoMichard/TrackMyAssets#readme",
  "dependencies": {
    "@babel/core": "7.14.2",
    "@material-ui/core": "^4.12.3",
    "@nivo/bar": "^0.73.1",
    "@nivo/core": "^0.73.0",
    "@nivo/line": "^0.73.0",
    "@nivo/pie": "^0.73.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
