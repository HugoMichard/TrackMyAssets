    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "if-env": "^1.0.4",
    "jade": "~1.11.0",
    "jquery": "3.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
